{#
   {% include "../elements/button.twig" with {
      tag:'', a/ button
      type : '',
      style: '',
      value: 'some Text',
      shape: '', normal / circle
    }%}
#}
{% if shape == "normal" %}
  <{{ tag }} class="btn {{ type }} {{ style }}">{{ value }}</{{ tag }}>
{% elseif shape == "circle" %}
  <{{ tag }} class="btn {{ type }} {{ style }}">{{ value }} {{ icon }}</{{ tag }}>
{% elseif submit == true %}
  <{{ tag }} type="submit" class="btn-eq {{ type }} {{ style }}">{{ icon }}</{{ tag }}>
{% endif %}
