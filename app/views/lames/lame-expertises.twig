<section class="lame lame-expertises">
  <div class="lame-expertises__section">
    {% for item in items %}
      {% if item.header %}
        <ul class="lame-expertises__section__social">
          <li class="lame-expertises__section__social__ic collap"><i class="icon-icon-{{ item.leftPosition }}"></i></li>
          {% for item in item.icon %}
            <li class="lame-expertises__section__social__ic"><a href="#"><i class="icon-icon-{{ item.icon }}"></i></a></li>
          {% endfor %}
        </ul>
      {% endif %}
      {% if item.hasSubtitle %}
        <h2 class="lame-expertises__section__subTitle animated zoomIn">Expertises</h2>
      {% endif %}
      {% if item.hasColumn %}
        <div class="lame-expertises__section__content">
          <div class="row">
            {% for col in item.columns %}
              <div class="col-md-4 col-xs-12">
                <div class="cols">
                  <i class="cols__ic icon-icon-{{ col.icon }}"></i>
                  <h2 class="cols__title animated zoomIn">{{ col.contentH2 }}</h2>
                  <ul class="cols__items animated zoomIn">
                    {% for item in col.item %}
                      <li class="cols__item animated zoomIn">{{ item.content }}</li>
                    {% endfor %}
                  </ul>
                </div>
              </div>
            {% endfor %}
          </div>
        </div>
      {% endif %}
    {% endfor %}
  </div>
</section>