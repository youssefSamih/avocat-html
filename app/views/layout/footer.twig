<footer class="footer">
    {% for item in items %}
        <div class="{{ item.classDiv }}">
        {% if item.hasNoChild %}
                <i class="footer__el__ic icon-icon-clock"></i>
                <h2 class="footer__el__title"><p class="footer__responsive">Besoin d'aide</p><span>?</span></h2>
        {% endif %}
        {% for child in item.child %}
            <div class="{{ child.classChild }}">
                {% if child.hasNoChild %}
                    <i class="footer__el__ic icon-icon-close"></i>
                {% endif %}
                {% for item in child.items %}
                    <div class="footer__el__item itemNotoggle">
                        <i class="footer__el__ic icon-icon-{{ item.icon }}"></i>
                        <h2 class="footer__el__title"><p>{{ item.content }}</p></h2>
                    </div>
                {% endfor %}
            </div>
        {% endfor %}
        </div>
    {% endfor %}
</footer>